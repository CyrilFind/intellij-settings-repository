<application>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="14" />
    <option name="FONT_FAMILY" value="Source Code Pro for Powerline" />
    <option name="USE_LIGATURES" value="true" />
    <option name="SECONDARY_FONT_FAMILY" value="Roboto Mono for Powerline" />
  </component>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="MAIN_EDITOR:CONSOLE" />
  </component>
  <component name="JetEditorOptions">
    <option name="donTShowConversionDialog" value="true" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
  </component>
  <component name="XmlEditorOptions">
    <option name="insertQuotesForAttributeValue" value="true" />
  </component>
</application>